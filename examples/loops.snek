// Simple while loop
mut i = 1;
while i <= 5 {
    Sys.println(i);

    i = i + 1;
}

Sys.println("");

// For loop - Array
for let v in [1, 2, 3, 4, 5] {
    Sys.println(v);
}

Sys.println("");

// For loop - Hash
let myHash = {
    axolotl: 1 + 2,
    string: "hello"
};
for let [value, key] in myHash {
    Sys.println(key >< " " >< value);
}