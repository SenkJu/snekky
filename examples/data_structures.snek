// Simple array
let simple_array = [1, "Axolotl", 2.3];
Sys.println(simple_array[1]);
simple_array[1] = "Gods";
simple_array.push(42);
Sys.println(simple_array.length());

// Arrays may contain any expression
let complex_array = [
    func() {
        2 + 3
    },
    42,
    3.4,
    [ "Another array" ],
    {
        my_key: 5
    }
];

// Hashes contain key-value-pairs
let my_hash = {
    my_func: func(a) {
        a + 7
    },
    number: 23
};

Sys.println(my_hash.my_func(my_hash.number));